            <key key="advertising" value="Placeholder"/>
        </item>

        <item name="Placeholder" type="node" icon="https://raw.githubusercontent.com/catgirlseraid/IDEditorPresets/main/icons/file-question.svg">
            <label text="Placeholder"/>
